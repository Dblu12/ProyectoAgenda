<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dam.listadecontactos" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditarTelefonosContacto"
            android:label="@string/title_activity_nuevo_contacto" >
        </activity>
        <activity
            android:name=".EditarContacto"
            android:label="@string/title_activity_editar_contacto" >
        </activity>
        <activity
            android:name=".AnadirContacto"
            android:label="@string/title_activity_anadir_contacto" >
        </activity>
    </application>

</manifest>
